{
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "AVA Current File",
                "program": "${workspaceRoot}/node_modules/ava/profile.js",
                "args": [
                    "${file}"
                ],
                "skipFiles": [
                    "<node_internals>/**/*.js"
                ]
            },
            {
                "name": "Attach to Node (restart)",
                "type": "node",
                "request": "attach",
                "restart": true,
                "port": 9229
            }
        ],
        "compounds": []
    },
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "editor.minimap.enabled": false,
    "editor.formatOnSave": false,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "gitlens.keymap": "alternate",
    "editor.fontSize": 15,
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.shell.osx": "/Users/willhoag/Code/scripts/code-shell",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontWeight": "100",
    "terminal.integrated.fontWeightBold": "300",
    "cSpell.userWords": [
        "SMTP",
        "Webhook",
        "deserialize",
        "loggable",
        "promisify",
        "ratelimit",
        "shopify",
        "signup",
        "theming",
        "webhooks"
    ],
    "zenMode.fullScreen": false,
    "zenMode.hideStatusBar": false,
    "gitlens.currentLine.enabled": false,
    "code-runner.showRunIconInEditorTitleMenu": false,
    "editor.tabSize": 2,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-w>",
                "s"
            ],
            "after": [],
            "commands": [
                {
                    "command": ":sp",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "=",
                "="
            ],
            "commands": [
                {
                    "command": "editor.action.reindentselectedlines",
                }
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "="
            ],
            "commands": [
                {
                    "command": "editor.action.reindentselectedlines",
                }
            ]
        }
    ],
    "explorer.confirmDragAndDrop": false,
    "markdown-preview-enhanced.liveUpdate": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editor.tabSizing": "shrink",
    "workbench.statusBar.visible": true,
    "colorHelper.pickerForm": "simple",
    "colorHelper.disableTransparent": true,
    "colorHelper.resident": true,
    "editor.minimap.renderCharacters": false,
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "material-icon-theme",
    "gitlens.codeLens.enabled": false,
    "code-runner.runInTerminal": true,
    "code-runner.preserveFocus": false,
    "debug.node.autoAttach": "on",
    "html-css-class-completion.excludeGlobPattern": "node_modules",
    "vim.foldfix": true,
    "vim.sneak": true,
    "yaml.format.enable": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggestSelection": "first",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "explorer.confirmDelete": false,
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.filtering.excludeGlobs": [
        "**/node_modules/**/*"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "gitlens.menus": {
        "editorGroup": {
            "blame": false,
            "compare": false
        }
    },
    "html.mirrorCursorOnMatchingTag": false,
    "window.zoomLevel": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "markdown-pdf.breaks": true,
    "markdown-pdf.displayHeaderFooter": false,
    "editor.wordWrap": "on",
    "emmet.triggerExpansionOnTab": true,
    "redhat.telemetry.enabled": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "settingsSync.ignoredSettings": [
        "terminal.integrated.automationShell.osx",
        "terminal.integrated.fontFamily"
    ],
    "terminal.integrated.fontFamily": "MesloLGS Nerd Font",
    "html.format.wrapLineLength": 0,
    "vim.neovimPath": "/opt/homebrew/bin/nvim",
    "liveServer.settings.donotVerifyTags": true,
    "workbench.activityBar.visible": false,
    "debug.javascript.autoAttachFilter": "onlyWithFlag"
}