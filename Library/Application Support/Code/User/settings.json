{
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Attach to Node (restart)",
                "type": "node",
                "request": "attach",
                "restart": true,
                "port": 9229
            }
        ],
        "compounds": []
    },
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "editor.minimap.enabled": false,
    "prettier.singleQuote": true,
    "gitlens.keymap": "alternate",
    "editor.fontSize": 15,
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontWeight": "100",
    "terminal.integrated.fontWeightBold": "300",
    "cSpell.userWords": [
        "SMTP",
        "Webhook",
        "deserialize",
        "loggable",
        "promisify",
        "ratelimit",
        "shopify",
        "signup",
        "webhooks"
    ],
    "gitlens.currentLine.enabled": false,
    "code-runner.showRunIconInEditorTitleMenu": false,
    "editor.tabSize": 2,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-w>",
                "s"
            ],
            "after": [],
            "commands": [
                {
                    "command": ":sp",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "=",
                "="
            ],
            "commands": [
                {
                    "command": "editor.action.reindentselectedlines"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "="
            ],
            "commands": [
                {
                    "command": "editor.action.reindentselectedlines"
                }
            ]
        }
    ],
    "explorer.confirmDragAndDrop": false,
    "markdown-preview-enhanced.liveUpdate": false,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editor.tabSizing": "shrink",
    "colorHelper.pickerForm": "simple",
    "colorHelper.disableTransparent": true,
    "colorHelper.resident": true,
    "editor.minimap.renderCharacters": false,
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "material-icon-theme",
    "gitlens.codeLens.enabled": false,
    "code-runner.runInTerminal": true,
    "code-runner.preserveFocus": false,
    "debug.node.autoAttach": "on",
    "html-css-class-completion.excludeGlobPattern": "node_modules",
    "vim.foldfix": true,
    "vim.sneak": true,
    "yaml.format.enable": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggestSelection": "first",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "explorer.confirmDelete": false,
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.filtering.excludeGlobs": [
        "**/node_modules/**/*"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "gitlens.menus": {
        "editorGroup": {
            "blame": false,
            "compare": false
        }
    },
    "html.mirrorCursorOnMatchingTag": false,
    "window.zoomLevel": 0,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "markdown-pdf.breaks": true,
    "markdown-pdf.displayHeaderFooter": false,
    "editor.wordWrap": "on",
    "emmet.triggerExpansionOnTab": true,
    "redhat.telemetry.enabled": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "settingsSync.ignoredSettings": [
        "terminal.integrated.automationShell.osx",
        "terminal.integrated.fontFamily"
    ],
    "terminal.integrated.fontFamily": "MesloLGS Nerd Font",
    "html.format.wrapLineLength": 0,
    "vim.neovimPath": "/opt/homebrew/bin/nvim",
    "liveServer.settings.donotVerifyTags": true,
    "debug.javascript.autoAttachFilter": "onlyWithFlag",
    "todo-tree.general.statusBar": "current file",
    "todo-tree.highlights.enabled": false,
    "MinifyAll.terserMinifyOptions": {
        "mangle": false,
        "compress": {
            "drop_console": true,
            "dead_code": false,
            "keep_fnames": false,
            "keep_classnames": false
        }
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveServer.settings.host": "localhost",
    "emmet.includeLanguages": {
        "mdx": "html"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "yaml": false
    },
    "editor.inlineSuggest.enabled": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.codeActionsOnSave.rules": null,
    "[liquid]": {
        "editor.defaultFormatter": "Shopify.theme-check-vscode"
    },
    "git.openRepositoryInParentFolders": "always",
    "tailwind-fold.autoFold": false,
    "workbench.editor.tabActionCloseVisibility": false,
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "code-shell": {
            "path": "code-shell",
            "icon": "terminal-tmux",
            "overrideName": true
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.osx": "code-shell",
    "github.copilot.editor.enableAutoCompletions": true,
    "supermaven.otherWarning": false,
    "window.confirmSaveUntitledWorkspace": false,
    "window.customTitleBarVisibility": "windowed",
    "gitlens.views.commitDetails.files.layout": "list",
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
    "geminicodeassist.project": "",
    "zenMode.showTabs": "none",
    "editor.accessibilitySupport": "off",
    "git.blame.editorDecoration.enabled": false,
    "git.blame.statusBarItem.enabled": true
}