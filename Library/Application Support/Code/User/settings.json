{
    "launch": {
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "AVA Current File",
                "program": "${workspaceRoot}/node_modules/ava/profile.js",
                "args": [
                    "${file}"
                ],
                "skipFiles": [
                    "<node_internals>/**/*.js"
                ]
            }
        ],
        "compounds": []
    },
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true,
        "suppressShowKeyBindingsNotice": true,
        "suppressWelcomeNotice": true
    },
    "guides.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.minimap.enabled": false,
    "editor.formatOnSave": false,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "gitlens.keymap": "alternate",
    "vsicons.presets.hideFolders": true,
    "editor.fontSize": 15,
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.shell.osx": "/usr/local/bin/fish",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontWeight": "100",
    "terminal.integrated.fontWeightBold": "300",
    "cSpell.userWords": [
        "SMTP",
        "Webhook",
        "deserialize",
        "loggable",
        "promisify",
        "ratelimit",
        "signup",
        "theming",
        "webhooks"
    ],
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "zenMode.fullScreen": false,
    "zenMode.hideStatusBar": false,
    "gitlens.currentLine.enabled": false,
    "gitHistory.showEditorTitleMenuBarIcons": false,
    "code-runner.showRunIconInEditorTitleMenu": false,
    "editor.tabSize": 2,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-w>",
                "s"
            ],
            "after": [],
            "commands": [
                {
                    "command": ":sp",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        }
    ],
    "explorer.confirmDragAndDrop": false,
    "markdown-preview-enhanced.liveUpdate": true,
    "workbench.activityBar.visible": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editor.tabSizing": "shrink",
    "workbench.statusBar.visible": true,
    "colorHelper.pickerForm": "simple",
    "colorHelper.disableTransparent": true,
    "colorHelper.resident": true,
    "editor.minimap.renderCharacters": false,
    "bracket-pair-colorizer-2.colorMode": "Independent",
    "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
    "bracket-pair-colorizer-2.activeScopeCSS": [
        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : {color}; opacity: 0.2",
        "backgroundColor : {color}"
    ],
    "bracket-pair-colorizer-2.scopeLineCSS": [
        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : {color}; opacity: 0.4"
    ],
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "material-icon-theme",
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/willhoag/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.draft-path": "/Users/willhoag/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.minikube-path": "/Users/willhoag/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "gitlens.codeLens.enabled": false,
    "code-runner.runInTerminal": true,
    "code-runner.preserveFocus": false,
    "debug.node.autoAttach": "on",
    "html-css-class-completion.excludeGlobPattern": "node_modules",
    "vim.foldfix": true,
    "vim.sneak": true,
    "yaml.format.enable": true,
    "cloudcode.gke": {
        "projectIds": [
            "cardsaholic"
        ]
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDelete": false,
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "todo-tree.filtering.excludeGlobs": [
        "**/node_modules"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "gitlens.menus": {
        "editorGroup": {
            "blame": false,
            "compare": false
        }
    },
    "html.mirrorCursorOnMatchingTag": false,
    "todo-tree.tree.showScanModeButton": false,
    "terminal.external.osxExec": "iTerm.app"
}